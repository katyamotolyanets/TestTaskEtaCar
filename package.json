{
  "scripts": {
    "frontend": "cd frontend && npm i && npm start",
    "backend": "cd server && npm i && npm start",
    "tsc": "./node_modules/typescript/bin/tsc",
    "postinstall": "npm run tsc",
    "start": "concurrently \"npm run backend\" \"npm run frontend\""
  },
  "dependencies": {
    "concurrently": "^7.5.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.8.4"
  }
}
